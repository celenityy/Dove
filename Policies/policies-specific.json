{
    "policies": {
        "Preferences": {
            "app.update.auto": {
                "Value": true,
                "Status": "default"
            }
        },
        "SearchEngines": {
            "Add": [
                {
                    "Name": "Brave",
                    "URLTemplate": "https://search.brave.com/search?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://cdn.search.brave.com/serp/v1/static/brand/eebf5f2ce06b0b0ee6bbd72d7e18621d4618b9663471d42463c692d019068072-brave-lion-favicon.png",
                    "Alias": "brave",
                    "Description": "Brave Search: private, independent, open",
                    "SuggestURLTemplate": "https://search.brave.com/api/suggest?q={searchTerms}"
                },
                {
                    "Name": "DuckDuckGo (HTML)",
                    "URLTemplate": "https://html.duckduckgo.com/html/?q={searchTerms}",
                    "Method": "POST",
                    "IconURL": "https://duckduckgo.com/favicon.ico",
                    "Alias": "ddgh",
                    "Description": "Search DuckDuckGo (HTML)",
                    "SuggestURLTemplate": "https://duckduckgo.com/ac/?q={searchTerms}&type=list"
                },
                {
                    "Name": "DuckDuckGo (Lite)",
                    "URLTemplate": "https://lite.duckduckgo.com/lite/?q={searchTerms}",
                    "Method": "POST",
                    "IconURL": "https://duckduckgo.com/favicon.ico",
                    "Alias": "ddgl",
                    "Description": "Search DuckDuckGo (Lite)",
                    "SuggestURLTemplate": "https://duckduckgo.com/ac/?q={searchTerms}&type=list"
                },
                {
                    "Name": "Ecosia",
                    "URLTemplate": "https://www.ecosia.org/search?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://www.ecosia.org/static/icons/favicon.ico",
                    "Alias": "eco",
                    "Description": "Search Ecosia",
                    "SuggestURLTemplate": "https://ac.ecosia.org/autocomplete?q={searchTerms}&type=list"
                },
                {
                    "Name": "Kagi",
                    "URLTemplate": "https://kagi.com/search?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://kagi.com/favicon.ico",
                    "Alias": "kagi",
                    "Description": "A privacy-focused, user-centric search engine.",
                    "SuggestURLTemplate": "https://kagi.com/api/autosuggest?q={searchTerms}"
                },
                {
                    "Name": "Kagi (HTML)",
                    "URLTemplate": "https://kagi.com/html/search?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://kagi.com/favicon.ico",
                    "Alias": "kagi",
                    "Description": "A privacy-focused, user-centric search engine.",
                    "SuggestURLTemplate": "https://kagi.com/api/autosuggest?q={searchTerms}"
                },
                {
                    "Name": "Mojeek",
                    "URLTemplate": "https://www.mojeek.com/search?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://www.mojeek.com/favicon.png",
                    "Alias": "mojeek",
                    "Description": "Search the web using Mojeek"
                },
                {
                    "Name": "Mullvad Leta (Brave)",
                    "URLTemplate": "https://leta.mullvad.net/?q={searchTerms}&engine=brave",
                    "Method": "GET",
                    "IconURL": "https://mullvad.net/favicon.ico",
                    "Alias": "leta",
                    "Description": "A privacy focused search engine provided by Mullvad for customers of our VPN."
                },
                {
                    "Name": "Mullvad Leta (Google)",
                    "URLTemplate": "https://leta.mullvad.net/?q={searchTerms}&engine=google",
                    "Method": "GET",
                    "IconURL": "https://mullvad.net/favicon.ico",
                    "Alias": "leta",
                    "Description": "A privacy focused search engine provided by Mullvad for customers of our VPN."
                },
                {
                    "Name": "No Search",
                    "URLTemplate": "https://nosearch.invalid/{searchTerms}",
                    "Method": "GET",
                    "Alias": "nosearch",
                    "Description": "Just a placeholder for users who don't wish to set a default search engine. :)"
                },
                {
                    "Name": "Qwant",
                    "URLTemplate": "https://www.qwant.com?q={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://www.qwant.com/public/favicon.066f5ee2ab77b590bb5846c32c57cb84.ico",
                    "Alias": "qwant",
                    "Description": "Qwant The search engine that doesn't know anything about you.",
                    "SuggestURLTemplate": "https://api.qwant.com/v3/suggest/?q={searchTerms}"
                },
                {
                    "Name": "Startpage",
                    "URLTemplate": "https://www.startpage.com/sp/search?query={searchTerms}",
                    "Method": "GET",
                    "IconURL": "https://www.startpage.com/favicon.ico",
                    "Alias": "sp",
                    "Description": "Startpage Search",
                    "SuggestURLTemplate": "https://www.startpage.com/osuggestions?q={searchTerms}"
                }
            ],
            "Default": "Brave",
            "PreventInstalls": false,
            "Remove": [
                "Google",
                "Bing",
                "Amazon.com"
            ]
        }
    }
}